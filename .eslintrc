{
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended",
    "plugin:react/recommended",
    "plugin:react-hooks/recommended",
  ],
  "env": { "browser": true, "node": true, "es6": true },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
    },
    "ecmaVersion": "latest",
  },
  "plugins": ["react"],
  "rules": {
    "eqeqeq": "error",
    "no-used-vars": "false",
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto",
      },
    ],
  },
}
